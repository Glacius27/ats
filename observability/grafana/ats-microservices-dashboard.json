{
  "dashboard": {
    "title": "ATS Microservices - Prometheus & .NET Metrics",
    "tags": ["ats", "microservices", "prometheus", "dotnet"],
    "timezone": "browser",
    "refresh": "10s",
    "schemaVersion": 38,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Requests Per Second (RPS) - All Services",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_received_total{job=~\"$service\"}[5m])) by (job, code)",
            "legendFormat": "{{job}} - {{code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "min": 0
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "showLegend": true}
        }
      },
      {
        "id": 2,
        "title": "Error Rate (4xx + 5xx) - req/s",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_received_total{job=~\"$service\",code=~\"5..\"}[5m])) by (job)",
            "legendFormat": "{{job}} - 5xx",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_requests_received_total{job=~\"$service\",code=~\"4..\"}[5m])) by (job)",
            "legendFormat": "{{job}} - 4xx",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "min": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "/5xx/"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            },
            {
              "matcher": {"id": "byRegexp", "options": "/4xx/"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            }
          ]
        }
      },
      {
        "id": 3,
        "title": "HTTP Request Duration - p95, p99 (seconds)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (job, le))",
            "legendFormat": "{{job}} - p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=~\"$service\"}[5m])) by (job, le))",
            "legendFormat": "{{job}} - p99",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"},
            "min": 0
          }
        }
      },
      {
        "id": 4,
        "title": "Active HTTP Requests",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(http_requests_in_progress{job=~\"$service\"}) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"},
            "min": 0
          }
        }
      },
      {
        "id": 5,
        "title": "Total Requests (Counter)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(increase(http_requests_received_total{job=~\"$service\"}[1h])) by (job, code)",
            "legendFormat": "{{job}} - {{code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"},
            "min": 0
          }
        }
      },
      {
        "id": 6,
        "title": "Error Rate Percentage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "(sum(rate(http_requests_received_total{job=~\"$service\",code=~\"[45]..\"}[5m])) by (job) / sum(rate(http_requests_received_total{job=~\"$service\"}[5m])) by (job)) * 100",
            "legendFormat": "{{job}} - Error %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {"mode": "palette-classic"},
            "min": 0,
            "max": 100
          },
          "overrides": [
            {
              "matcher": {"id": "byThresholds"},
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"value": null, "color": "green"},
                      {"value": 1, "color": "yellow"},
                      {"value": 5, "color": "red"}
                    ]
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "id": 7,
        "title": "Requests by Status Code",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(http_requests_received_total{job=~\"$service\"}[5m])) by (job, code)",
            "legendFormat": "{{job}} - {{code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "thresholds"},
            "min": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byRegexp", "options": "/2../"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            },
            {
              "matcher": {"id": "byRegexp", "options": "/4../"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            },
            {
              "matcher": {"id": "byRegexp", "options": "/5../"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            }
          ]
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 8,
        "title": ".NET Process Memory (bytes)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "sum(dotnet_total_memory_bytes{job=~\"$service\"}) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {"mode": "palette-classic"},
            "min": 0
          }
        }
      },
      {
        "id": 9,
        "title": ".NET GC Collections",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "sum(rate(dotnet_collection_count_total{job=~\"$service\"}[5m])) by (job, generation)",
            "legendFormat": "{{job}} - Gen {{generation}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "palette-classic"},
            "min": 0
          }
        }
      },
      {
        "id": 10,
        "title": "RPS by Service (Summary)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "sum(rate(http_requests_received_total{job=~\"$service\"}[5m])) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "thresholds"},
            "min": 0
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        }
      },
      {
        "id": 11,
        "title": "Error Rate by Service (Summary)",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 40},
        "targets": [
          {
            "expr": "sum(rate(http_requests_received_total{job=~\"$service\",code=~\"[45]..\"}[5m])) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "thresholds"},
            "min": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byThresholds"},
              "properties": [
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      {"value": null, "color": "green"},
                      {"value": 0.1, "color": "yellow"},
                      {"value": 1, "color": "red"}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        }
      },
      {
        "id": 12,
        "title": "Service Health Status",
        "type": "stat",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 40},
        "targets": [
          {
            "expr": "up{job=~\"ats-.*-service\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "none",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        }
      }
    ],
    "templating": {
      "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": {"type": "prometheus", "uid": "${datasource}"},
        "query": "label_values(http_requests_received_total, job)",
        "current": {"selected": false, "text": "All", "value": "$__all"},
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "regex": "/ats-(.*)-service/",
        "useTags": false
      },
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {"selected": true, "text": "Prometheus", "value": "Prometheus"}
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
